<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="DB_IP" connectionString="Data Source=win2008r2;Initial Catalog=<setup_your_catalog>;Integrated Security=False;User ID=<userid>;Password=<password>;Connect Timeout=0;Encrypt=False;TrustServerCertificate=False"/>
  </connectionStrings>
  <appSettings>
    <add key="db-ip-file-path" value="<path_to_your_file->\dbip-city.csv"/>
  </appSettings>
</configuration>